{
  "extends": ["react-app", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "no-unused-vars": "error",
    "prefer-const": "error",
    "react-hooks/exhaustive-deps": "error",
    "react/no-unused-prop-types": "error",
    "react/prop-types": "error",
    "react/require-default-props": "error",
    "require-await": "error"
  }
}
